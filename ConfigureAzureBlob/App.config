<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <add key="BlobEndpoint" value="https://airdye.blob.core.windows.net/" />

    <add key="QueueEndpoint" value="https://airdye.queue.core.windows.net/" />
    <add key="TableEndpoint" value="https://airdye.table.core.windows.net/" />
    <add key="AccountName" value="airdye" />
    <add key="AccountKey" value="your account key here" />
